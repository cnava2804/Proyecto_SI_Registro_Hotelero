@model Proyecto_SI_Registro_Hotelero.Models.PisoHabitacion

@{
    ViewData["Title"] = "Delete";
}

<h1>Borrar</h1>

<h3>¿Estás seguro de borrar piso de la habitación?</h3>
<div>
    
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Piso)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Piso)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="PisoHId" />
        <input type="submit" class="btn btn-danger" value="Borrar" />
        <a asp-action="Index" class="btn btn-warning">Regresar</a>
    </form>
</div>
