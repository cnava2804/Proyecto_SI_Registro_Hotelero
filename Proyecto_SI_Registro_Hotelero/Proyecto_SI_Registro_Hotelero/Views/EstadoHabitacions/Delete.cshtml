@model Proyecto_SI_Registro_Hotelero.Models.EstadoHabitacion

@{
    ViewData["Title"] = "Delete";
}

<body id="Imag">

    <h3><b>¿Estás seguro de borrar habitación?</b></h3>
    <div class="container" style="background-color:white; border-radius:15px; ">

        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Estado)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Estado)
            </dd>
        </dl>
        </div>
    <div>
        <form asp-action="Delete">
            <input type="hidden" asp-for="EstadoHId" />
            <input type="submit" class="btn btn-outline-danger" value="Borrar" />
            <a asp-action="Index" class="btn btn-warning">Regresar</a>
        </form>
    </div>
</body>